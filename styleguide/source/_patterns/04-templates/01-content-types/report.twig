
{# site header #}
{% block header %}{% endblock %}

<main id="main-content" class="ma__policy-advisory" tabindex="-1">
  <div class="pre-content">
    {% block preContent %}
      {% include "@organisms/by-template/illustrated-header.twig" %}
    {% endblock %}
  </div>
    <div class="main-content main-content--two ma__policy-advisory__top-contact">
      <div class="page-content">
        {% if sideContent.contactList %}
          {% set contactList = sideContent.contactList %}
          {% include "@organisms/by-author/contact-list.twig" %}
        {% endif %}
      </div>
    </div>
  <div class="ma__policy-advisory__content">
    {% if jumpLinks %}
      {% include "@organisms/by-template/jump-links.twig" %}
    {% endif %}
    <div class="main-content main-content--two">
      <div class="page-content">
        {% for richText in mainContent.contents %}
          {% include "@organisms/by-author/rich-text.twig" %}
        {% endfor %}


        <div class="ma__iframe ">
          <div class="ma__iframe__container js-ma-responsive-iframe">
            <iframe height="400" src="http://superset-sandbox.digital.mass.gov/superset/explore/table/11/?form_data=%7B%22datasource%22%3A%2211__table%22%2C%22viz_type%22%3A%22area%22%2C%22slice_id%22%3A73%2C%22granularity_sqla%22%3A%22month%22%2C%22time_grain_sqla%22%3A%22month%22%2C%22since%22%3A%222+years+ago%22%2C%22until%22%3A%22now%22%2C%22metrics%22%3A%5B%22sum__active_patients%22%2C%22sum__certified_patients%22%5D%2C%22groupby%22%3A%5B%5D%2C%22limit%22%3A50%2C%22timeseries_limit_metric%22%3Anull%2C%22show_brush%22%3Afalse%2C%22show_legend%22%3Atrue%2C%22line_interpolation%22%3A%22linear%22%2C%22stacked_style%22%3A%22stack%22%2C%22rich_tooltip%22%3Atrue%2C%22contribution%22%3Afalse%2C%22show_controls%22%3Afalse%2C%22x_axis_format%22%3A%22smart_date%22%2C%22x_axis_showminmax%22%3Atrue%2C%22y_axis_format%22%3Anull%2C%22y_axis_bounds%22%3A%5Bnull%2Cnull%5D%2C%22y_log_scale%22%3Afalse%2C%22rolling_type%22%3A%22None%22%2C%22time_compare%22%3Anull%2C%22num_period_compare%22%3A%22%22%2C%22period_ratio_type%22%3A%22growth%22%2C%22resample_how%22%3Anull%2C%22resample_rule%22%3Anull%2C%22resample_fillmethod%22%3Anull%2C%22where%22%3A%22%22%2C%22having%22%3A%22%22%2C%22filters%22%3A%5B%5D%7D&amp;standalone=true&amp;height=400" allowfullscreen="" title=""></iframe>
              </div>
        </div>


        {% if mainContent.iframe %}
          {% set video = mainContent.iframe %}
          {% include "@atoms/09-media/iframe.twig" %}
        {% endif %}

        {% if mainContent.figure %}
          {% set figure = mainContent.figure %}
          {% include "@atoms/09-media/figure.twig" %}
        {% endif %}

        <!-- {% if mainContent.video %}
          {% set video = mainContent.video %}
          {% include "@atoms/09-media/video.twig" %}
        {% endif %} -->

        {% block pageContent %}
          {% include "@base/placeholder.twig" with {'placeholder':{'text':'Page Content'}} %}
        {% endblock %}

        {% if mainContent.formDownloads %}
          {% set formDownloads = mainContent.formDownloads %}
          {% include "@organisms/by-author/form-downloads.twig" %}
        {% endif %}

        {% if mainContent.contactList %}
          {% set contactList = mainContent.contactList %}
          {% include "@organisms/by-author/contact-list.twig" %}
        {% endif %}

        {% if mainContent.footnoteList %}
          {% set footnoteList = mainContent.footnoteList %}
          {% include "@organisms/by-author/footnote-list.twig" %}
        {% endif %}
      </div>
      <aside class="sidebar {{ sidebar.coloredHeadings ? 'sidebar--colored' : '' }}">
        {% if sideContent.contactList %}
          {% set contactList = sideContent.contactList %}
          {% include "@organisms/by-author/contact-list.twig" %}
        {% endif %}

        {% if sideContent.pressListing %}
          {% set pressListing = sideContent.pressListing %}
          {% include "@organisms/by-author/press-listing.twig" %}
        {% endif %}

        {% if sideContent.eventListing %}
          {% set eventListing = sideContent.eventListing %}
          {% include "@organisms/by-author/event-listing.twig" %}
        {% endif %}

        {% block sidebar %}
          {% include "@base/placeholder.twig" with {'placeholder':{'text':'sidebar'}} %}
        {% endblock %}
      </aside>
    </div>
  </div>
  <div class="post-content">
    {% block postContent %}
      {% set backButton = postContent.backButton %}
      {% include "@molecules/back-button.twig" %}
      {% set pageFlipper = postContent.pageFlipper %}
      {% include "@organisms/by-author/page-flipper.twig" %}
    {% endblock %}
  </div>
  {% block schemaContent %}{% endblock %}
</main>

{# site footer #}
{% block footer %}{% endblock %}
